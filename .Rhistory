llimy <- 0;  llimx=0
ulimy <-  2    ; ulimx=1
f <- function(x,y) 1/6*(x^2*y+x*y^2)
integrate(function(y) {
sapply(y, function(y) {
integrate(function(x) f(x,y), llimx, ulimx)$value
})
}, llimy, ulimy)
